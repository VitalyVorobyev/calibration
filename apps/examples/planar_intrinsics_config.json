{
  "algorithm": "planar",
  "options": {
    "min_corners_per_view": 150,
    "refine": true,
    "estim_options": {
      "use_skew": false,
      "homography_ransac": {
        "max_iters": 4000,
        "thresh": 1.0,
        "min_inliers": 50,
        "confidence": 0.99,
        "seed": 42,
        "refit_on_inliers": true
      }
    },
    "optim_options": {
      "core": {
        "optimizer": "default",
        "huber_delta": 2.0,
        "epsilon": 1e-9,
        "max_iterations": 1000,
        "compute_covariance": true,
        "verbose": false
      },
      "num_radial": 3,
      "optimize_skew": true,
      "fixed_distortion_indices": [],
      "fixed_distortion_values": []
    }
  },
  "cameras": [
    {
      "camera_id": "cam0",
      "model": "pinhole_brown_conrady",
      "image_size": [2048, 1536]
    }
  ]
}
