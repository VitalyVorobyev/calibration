{
  "session": {
    "id": "charuco_cam1_demo",
    "description": "Intrinsic calibration example for the bundled ChArUco detections."
  },
  "calibration": {
    "type": "intrinsics",
    "algorithm": "charuco_planar",
    "options": {
      "min_corners_per_view": 150,
      "refine": true,
      "optimize_skew": false,
      "num_radial": 3,
      "huber_delta": 2.0,
      "max_iterations": 200,
      "epsilon": 1e-9,
      "point_scale": 10.0,
      "auto_center_points": true,
      "homography_ransac": {
        "max_iters": 4000,
        "thresh": 1.0,
        "min_inliers": 50,
        "confidence": 0.995
      }
    }
  },
  "cameras": [
    {
      "camera_id": "cam1",
      "model": "pinhole_brown_conrady",
      "image_size": [2048, 1536]
    }
  ]
}
